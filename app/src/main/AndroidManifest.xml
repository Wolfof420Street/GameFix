<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wolf.gamefix">

    <application
        android:allowBackup="true"
        android:icon="@drawable/intro1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/intro1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StatsActivity"></activity>
        <activity android:name=".InfoActivity" />
        <activity android:name=".ResultsActivity" />
        <activity android:name=".FixturesActivity" />
        <activity android:name=".LeaguesActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyIntro"
            android:theme="@style/FullscreenTheme" />
    </application>

</manifest>